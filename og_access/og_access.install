/**
 * @file
 * Install, update, and uninstall functions for the Group access module.
 */

/**
 * Implement hook_uninstall().
 */
function og_access_uninstall() {
  $vars = array(
    'og_access_strict_private',
  );
  foreach ($vars as $var) {
    variable_del($var);
  }
}