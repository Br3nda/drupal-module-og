/**
 * @file
 * Install, update, and uninstall functions for the Group views module.
 */

/**
 * Implement hook_uninstall().
 */
function og_views_uninstall() {
  $vars = array(
    'og_views_global_view',
  );
  foreach ($vars as $var) {
    variable_del($var);
  }
}